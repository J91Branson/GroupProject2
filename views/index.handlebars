    <!--- Start Add Your Meme Section -->
   <div id="AddYourMeme" class="offset">
      <div class="container">
        <div id="form-buffer" class="bg-dark">

          <h1 class="bg-dark text-white form1">Add Your Meme</h1>
          <form class="bg-dark form2">
            <div class="form-group text-white">
              <label for="FormControlInput1">Meme Name</label>
              <input type="text" class="form-control" id="FormName">
            </div>
            <div class="form-group text-white">
              <label for="FormControlInput1">Meme URL</label>
              <input type="text" class="form-control" id="FormURL">
            </div>
            <div class="form-group text-white">
              <label for="exampleFormControlTextarea1">Description</label>
              <textarea class="form-control" id="FormDesc" rows="3"></textarea>
            </div>
            <div class="form-group text-white">
              <label for="FormControlSelect1">Catagory</label>
              <select class="form-control" id="FormCategory">
                <option>Political</option>
                <option>Dank</option>
                <option>Dank Christian</option>
                <option>Animal</option>
                <option>Facebook</option>
              </select>
              <div class="col-auto">
                <br>
                <button type="submit" class="btn btn-primary mb-2">Submit</button>
              </div>

          </form>
        </div>
      </div>
    </div>
    <!--- End Add Your Meme  Section -->

  

</div>
